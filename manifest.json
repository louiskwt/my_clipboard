{
    "manifest_version": 2,
    "name": "My Clipboard",
    "version": "0.1",
    "icons": {},
    "description": "A clipboard app for people who need to do a lot of copy and paste",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script.js"],
            "css": ["style.css"]
        }
    ],
    "permissions": ["activeTab", "storage"],
    "browser_action": {
        "default_popup": "popup.html"
    }
}